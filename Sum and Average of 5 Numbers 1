#include <stdio.h>

int main() {

    int n1, n2, n3, n4, n5;

    int sum;

    float average;

    scanf("%d %d %d %d %d", &n1, &n2, &n3, &n4, &n5);

    sum = n1 + n2 + n3 + n4 + n5;
    
    average = sum / 5.0;
    
    printf("Sum of the 5 number is : %d\n", sum);

    printf("Average of the 5 number is : %.1f\n", average);

    return 0;

}
