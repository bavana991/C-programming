#include <stdio.h>

int main() {

    int n, i, num;

    float posCount = 0, negCount = 0;

    scanf("%d", &n);

    for (i = 0; i < n; i++) {

        scanf("%d", &num);

        if (num > 0)

            posCount++;

        else if (num < 0)

            negCount++;

    }

    printf("Count of Positive Integer is %.2f\n", posCount);

    printf("Count of Negative Integer is %.2f\n", negCount);

    return 0;

}
