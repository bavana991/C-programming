#include <stdio.h>

int main() {

    int n, i, search, found = 0;

    int arr[100];

    scanf("%d", &n);

    for (i = 0; i < n; i++) {

        scanf("%d", &arr[i]);

    }

    scanf("%d", &search);
    
    for (i = 0; i < n; i++) {

        if (arr[i] == search) {

            found = 1;

            break;

        }

    }

    if (found)

        printf("%d is presented in an array.\n", search);

    else

        printf("%d is not presented in an array.\n", search);

    return 0;

}
